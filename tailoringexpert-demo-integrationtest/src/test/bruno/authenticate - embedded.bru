meta {
  name: authenticate - embedded
  type: http
  seq: 4
}

post {
  url: {{HOST}}{{links.login}}
  body: json
  auth: inherit
}

body:json {
  {
    "userId": "{{USER}}",
    "password": "{{PASSWORD}}"
  }
}

script:post-response {
  let data = res.body;
  
  const auth = {};
  auth.accessToken = data.accessToken;
  auth.refreshToken = data.refreshToken;
  auth.refresh = data.links[0].href;
  
  bru.setVar("auth", auth);
  
}
